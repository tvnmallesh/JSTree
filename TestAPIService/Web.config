<configuration>
  <appSettings>
    <!-- MongoDb collections-->
    <add key="MongoDb:Host" value="localhost"/>
    <add key="MongoDb:Port" value="27017"/>
    <add key="MongoDb:UserName" value=""/>
    <add key="MongoDb:DatabaseName" value="test"/>
    <add key="Mongos:Collections:Projects" value="testcollection"/>
    <!--<add key="Mongos:Collections:Projects" value="ProjectCollection" />-->
    <!-- Enable Cors-->
    <add key="EnableCors:Origins" value="http://localhost:52180"/>
    <!--App links-->
    <add key="AppLinks:Home" value="https://Localhost/44403"/>
    <add key="AppLinks:Help" value="https://Localhost/"/>
    <add key="AppLinks:ServiceDesk" value="https://Localhost/"/>
    <add key="AppLinks:FAQ" value="https://Localhost/Frequently-Asked-Questions"/>
    <add key="AppLinks:PrivacyPolicy" value="https://Localhost"/>
    <add key="AppLinks:TermsOfUse" value="https://Localhost/"/>
    <add key="AppLinks:Disclaimer" value=" https://Localhost/Disclaimer"/>
    <add key="AppLinks:Copyright" value="https://Localhost/Copyright"/>
    <add key="AppLinks:ContactUs" value=""/>
    <add key="AppLinks:ProjectDetail" value="https://Localhost/Project/ProjectDetail"/>
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6.2"/>
    <httpRuntime targetFramework="4.6.1" maxUrlLength="5000"/>
  </system.web>
  <system.webServer>
        <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="true" /> 
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
    </handlers>
    
    <!--<httpProtocol>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*" />
        <add name="Access-Control-Allow-Headers" value="Content-Type" />
        <add name="Access-Control-Allow-Methods" value="GET,POST,PUT,DELETE,OPTIONS" />
      </customHeaders>
    </httpProtocol>-->
  </system.webServer>
